<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_plant"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_plant"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginScreen"
        tools:targetApi="31">
        <activity
            android:name=".Home.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".GetStarted"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".Home.HomeActivity"
            android:exported="false"
            />
        <activity
            android:name=".setting.SettingActivity"
            android:exported="false" />
        <activity
            android:name=".SignInPhoneNumber"
            android:exported="false" />
        <activity
            android:name=".SignUpPhoneNumber"
            android:exported="false" />
        <activity
            android:name=".SignInActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
        <meta-data
            android:name="com.facebook.sdk.ClientToken"
            android:value="@string/facebook_client_token" />
    </application>

</manifest>